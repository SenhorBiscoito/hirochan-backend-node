{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2020": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 11
    },
    "rules": {
        "no-unused-vars": [
            2,
            {
                "vars": "all",
                "args": "after-used"
            }
        ]
    }
}